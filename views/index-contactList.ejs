<!--Include header-->
<%- include ('include/_header-list') -%>
    <!--Close header-->
    <!--Include main-->
    <main id="site-main">
        <div class="container">
            <div class="box-nav d-flex-justify-between">
                <a href="/contact" class="border-shadow">
                    <span class="text-gradient">
                        New User <i class="fa-solid fa-user"></i>
                    </span>
                </a>
            </div>


            <!--Form handling-->
            <form action="/" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>Last name</th>
                            <th>Email</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%- include('include/_show-list') -%>
                    </tbody>
                </table>
            </form>
            <!--/Form handling-->

        </div>

    </main>
    <!--/Main-->


    <!--Include footer-->
    <%- include('include/_footer-list') -%>
        <!--Close footer-->